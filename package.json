{
  "name": "jm-config-server",
  "description": "jm config server",
  "license": "MIT",
  "author": "fefeding",
  "version": "4.0.4",
  "scripts": {
    "build": "easy build prod && npm run tsc",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "ci": "npm run lint && npm run cov && npm run tsc",
    "clean": "easy clean all && ets clean",
    "debug": "egg-bin debug -r egg-ts-helper/register",
    "deploy": "npm run clean && npm run build && tars-deploy ConfigWebServer",
    "dev": "egg-bin dev --port=8082 -r egg-ts-helper/register",
    "fix": "eslint -c ./.eslintrc.js --ext .vue,.js,.ts app/ --fix",
    "ii": "npm install --registry https://registry.npm.taobao.org",
    "kill": "easy kill",
    "lint": "eslint -c ./.eslintrc.js --ext .vue,.js,.ts app/",
    "lint:checked": "node_modules/.bin/eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "rebuild": "npm run clean && npm run build",
    "start": "egg-scripts start --port=8082 --daemon --workers=1 --title=egg-server-jv-config",
    "stop": "egg-scripts stop --title=egg-server-jv-config",
    "tsc": "ets && tsc -p tsconfig.json",
    "zip": "easy zip"
  },
  "dependencies": {
    "@tars/config": "^2.0.5",
    "@types/egg": "^1.5.0",
    "axios": "^0.19.2",
    "codemirror": "^5.54.0",
    "cos-nodejs-sdk-v5": "^2.9.15",
    "egg": "^2.3.0",
    "egg-cors": "^2.1.1",
    "egg-jproxy": "^1.0.3",
    "egg-view-vue-ssr": "^3.3.2",
    "egg-webpack": "^4.5.3",
    "egg-webpack-vue": "^2.0.0",
    "element-ui": "^2.13.1",
    "extend": "~3.0.0",
    "font-awesome": "^4.7.0",
    "form-data": "^4.0.0",
    "jm-egg-framework": "^1.0.10",
    "js-cookie": "^2.2.1",
    "koa-proxy": "^1.0.0-alpha.3",
    "lodash": "^4.17.4",
    "lodash-id": "^0.14.0",
    "lowdb": "^1.0.0",
    "mockjs": "^1.0.1-beta3",
    "moment": "^2.26.0",
    "mysql": "^2.18.1",
    "prettier-package-json": "^2.1.3",
    "qcloud-cos-sts": "^3.0.6",
    "reflect-metadata": "^0.1.13",
    "shortid": "^2.2.8",
    "showdown": "^1.8.6",
    "simplemde": "^1.11.2",
    "tslib": "^1.11.2",
    "typeorm": "^0.2.24",
    "v-jsoneditor": "^1.4.1",
    "vue": "^2.6.11",
    "vue-codemirror": "^4.0.6",
    "vue-property-decorator": "^7.2.0",
    "vue-router": "^3.1.6",
    "vuex": "^3.3.0",
    "vuex-class": "^0.3.1",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.150",
    "@types/lowdb": "^1.0.6",
    "@types/node": "^10.17.21",
    "@types/shortid": "^0.0.29",
    "cz-conventional-changelog": "^2.1.0",
    "easywebpack-cli": "^4.1.0",
    "easywebpack-vue": "^4.2.1",
    "egg-bin": "^4.9.0",
    "egg-scripts": "^2.13.0",
    "egg-ts-helper": "^1.25.8",
    "node-tool-utils": "^1.6.0",
    "ts-loader": "^5.3.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.9.1",
    "tslint-loader": "^3.5.3",
    "typescript": "^3.8.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "ci": {
    "version": "8, 10"
  },
  "egg": {
    "typescript": true,
    "framework": "jm-egg-framework"
  }
}
